int main()
  print 1;
  int i = 2;
  while i <= 45
  do
    bool a = (i / 15) > ((i - 1) / 15);
    bool b = (i / 3) > ((i - 1) / 3);
    bool c = (i / 5) > ((i - 1) / 5);
    if a then print "Fizz" + "Buzz"; 
    else 
      if b then print "Fizz";
      else
        if c then print "Buzz"; else print i; end
      end
    end
    i = i + 1;
  end
end

