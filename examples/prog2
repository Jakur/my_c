string main()
  //fizzbuzz without %
  string fizzbuzz ="";
  string fizz = "fizz";
  string buzz = "buzz";
  int rem = 0;
  
  int i = 1;
  while i <=100
  do
    //Gets modulo answer
    rem = remainder(i);
    
    //concatenates strings together
    if (rem == 15)
      then fizzbuzz = fizzbuzz + fizz + buzz + " ";
    end
    
    if (rem == 3)
      then fizzbuzz = fizzbuzz + fizz + " ";
    end
    
    if (rem == 5)
      then fizzbuzz = fizzbuzz + buzz + " ";
    end
    
    //should add the integer to the string
    if (rem == -1)
      then fizzbuzz = fizzbuzz + i + " ";
    end
    
  end
  
  print fizzbuzz;
  
end

//Determines if the current value is divisible by 3, 5, or 15
int remainder(int a)
  //for storing the remainders tested
  int threeRemainder = a;
  int fiveRemainder = a;
  
  while threeRemainder >= 3
  do
    threeRemainder = threeRemainder - 3;
  end
  
  while fiveRemainder >= 5
  do
    fiveRemainder = fiveRemainder - 5;
  end
  
  if (threeRemainder == 0 && fiveRemainder == 0)
    then return 15;
  end
  
  if (threeRemainder == 0)
    then return 3;
  end
  
  if (fiveRemainder == 0)
    then return 5;
  end
  
  return -1;
  
end
